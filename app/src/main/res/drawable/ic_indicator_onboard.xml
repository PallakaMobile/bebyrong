<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="oval">
            <solid android:color="@color/color3"/>
            <stroke android:width="@dimen/dp1" android:color="@color/color3"/>
            <size android:height="@dimen/default_margin" android:width="@dimen/default_margin"/>
        </shape>
    </item>
    <item android:state_selected="false">
        <shape android:shape="oval">
            <solid android:color="@color/gray"/>
            <!--<stroke android:width="@dimen/dp1" android:color="@color/white50"/>-->
            <size android:height="@dimen/default_margin" android:width="@dimen/dp1"/>
        </shape>
    </item>
</selector>
